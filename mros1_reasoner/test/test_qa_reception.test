<?xml version="1.0"?>
<launch>
  <arg name="onto" default="$(find mros1_reasoner)/owl/test1.owl"/>
  <arg name="tomasys" default="$(find mc_mdl_tomasys)/tomasys.owl"/>

  <node name="reasoner" pkg="mros1_reasoner" type="mros1_reasoner_node.py" output="screen">
    <param name="onto_file" type="string" value="$(arg onto)"/>
    <param name="tomasys_file" type="string" value="$(arg tomasys)"/>
  </node>
  <test test-name="test_qa_reception" pkg="mros1_reasoner" type="test_qa_reception.py"/>

</launch>
