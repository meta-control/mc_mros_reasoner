    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // General axioms
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    <rdf:Description>
        <rdf:type rdf:resource="http://www.w3.org/2002/07/owl#AllDisjointClasses"/>
        <owl:members rdf:parseType="Collection">
            <rdf:Description rdf:about="http://metacontrol.org/tomasys#Binding"/>
            <rdf:Description rdf:about="http://metacontrol.org/tomasys#Component"/>
            <rdf:Description rdf:about="http://metacontrol.org/tomasys#ComponentClass"/>
            <rdf:Description rdf:about="http://metacontrol.org/tomasys#Function"/>
            <rdf:Description rdf:about="http://metacontrol.org/tomasys#FunctionDesign"/>
            <rdf:Description rdf:about="http://metacontrol.org/tomasys#FunctionGrounding"/>
            <rdf:Description rdf:about="http://metacontrol.org/tomasys#Objective"/>
            <rdf:Description rdf:about="http://metacontrol.org/tomasys#QAvalue"/>
            <rdf:Description rdf:about="http://metacontrol.org/tomasys#QualityAttributeType"/>
            <rdf:Description rdf:about="http://metacontrol.org/tomasys#Role"/>
        </owl:members>
    </rdf:Description>
    


    <!-- 
    ///////////////////////////////////////////////////////////////////////////////////////
    //
    // Rules
    //
    ///////////////////////////////////////////////////////////////////////////////////////
     -->

    <rdf:Description rdf:about="http://metacontrol.org/tomasys#b">
        <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#Variable"/>
    </rdf:Description>
    <rdf:Description rdf:about="http://metacontrol.org/tomasys#r">
        <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#Variable"/>
    </rdf:Description>
    <rdf:Description rdf:about="http://metacontrol.org/tomasys#c">
        <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#Variable"/>
    </rdf:Description>
    <rdf:Description rdf:about="http://metacontrol.org/tomasys#cs">
        <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#Variable"/>
    </rdf:Description>
    <rdf:Description rdf:about="http://metacontrol.org/tomasys#fg">
        <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#Variable"/>
    </rdf:Description>
    <rdf:Description rdf:about="http://metacontrol.org/tomasys#cc">
        <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#Variable"/>
    </rdf:Description>
    <rdf:Description rdf:about="http://metacontrol.org/tomasys#fd">
        <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#Variable"/>
    </rdf:Description>
    <rdf:Description rdf:about="http://metacontrol.org/tomasys#o">
        <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#Variable"/>
    </rdf:Description>
    <rdf:Description>
        <swrla:isRuleEnabled rdf:datatype="http://www.w3.org/2001/XMLSchema#boolean">true</swrla:isRuleEnabled>
        <rdfs:comment rdf:datatype="http://www.w3.org/2001/XMLSchema#string">If a required objective by a FG is in ERROR -&gt; fg in error</rdfs:comment>
        <rdfs:label rdf:datatype="http://www.w3.org/2001/XMLSchema#string">S6b</rdfs:label>
        <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#Imp"/>
        <swrl:body>
            <rdf:Description>
                <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#AtomList"/>
                <rdf:first>
                    <rdf:Description>
                        <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#ClassAtom"/>
                        <swrl:classPredicate rdf:resource="http://metacontrol.org/tomasys#FunctionGrounding"/>
                        <swrl:argument1 rdf:resource="http://metacontrol.org/tomasys#fg"/>
                    </rdf:Description>
                </rdf:first>
                <rdf:rest>
                    <rdf:Description>
                        <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#AtomList"/>
                        <rdf:first>
                            <rdf:Description>
                                <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#DatavaluedPropertyAtom"/>
                                <swrl:propertyPredicate rdf:resource="http://metacontrol.org/tomasys#o_status"/>
                                <swrl:argument1 rdf:resource="http://metacontrol.org/tomasys#o"/>
                                <swrl:argument2 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">EXTERNAL_ERROR</swrl:argument2>
                            </rdf:Description>
                        </rdf:first>
                        <rdf:rest>
                            <rdf:Description>
                                <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#AtomList"/>
                                <rdf:first>
                                    <rdf:Description>
                                        <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#IndividualPropertyAtom"/>
                                        <swrl:propertyPredicate rdf:resource="http://metacontrol.org/tomasys#requiresO"/>
                                        <swrl:argument1 rdf:resource="http://metacontrol.org/tomasys#fg"/>
                                        <swrl:argument2 rdf:resource="http://metacontrol.org/tomasys#o"/>
                                    </rdf:Description>
                                </rdf:first>
                                <rdf:rest>
                                    <rdf:Description>
                                        <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#AtomList"/>
                                        <rdf:first>
                                            <rdf:Description>
                                                <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#ClassAtom"/>
                                                <swrl:classPredicate rdf:resource="http://metacontrol.org/tomasys#Objective"/>
                                                <swrl:argument1 rdf:resource="http://metacontrol.org/tomasys#o"/>
                                            </rdf:Description>
                                        </rdf:first>
                                        <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
                                    </rdf:Description>
                                </rdf:rest>
                            </rdf:Description>
                        </rdf:rest>
                    </rdf:Description>
                </rdf:rest>
            </rdf:Description>
        </swrl:body>
        <swrl:head>
            <rdf:Description>
                <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#AtomList"/>
                <rdf:first>
                    <rdf:Description>
                        <rdf:type rdf:resource="http://www.w3.org/2003/11/swrl#DatavaluedPropertyAtom"/>
                        <swrl:propertyPredicate rdf:resource="http://metacontrol.org/tomasys#fg_status"/>
                        <swrl:argument1 rdf:resource="http://metacontrol.org/tomasys#fg"/>
                        <swrl:argument2 rdf:datatype="http://www.w3.org/2001/XMLSchema#string">EXTERNAL_ERROR</swrl:argument2>
                    </rdf:Description>
                </rdf:first>
                <rdf:rest rdf:resource="http://www.w3.org/1999/02/22-rdf-syntax-ns#nil"/>
            </rdf:Description>
        </swrl:head>
    </rdf:Description>
